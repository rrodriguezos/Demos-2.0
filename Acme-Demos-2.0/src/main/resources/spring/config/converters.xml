<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd		
		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-4.0.xsd		
	">

	<util:list id="converters">

		<bean class="converters.StringToDeveloperConverter" />
		<bean class="converters.DeveloperToStringConverter" />
		
		<bean class="converters.StringToInvestorConverter" />
		<bean class="converters.InvestorToStringConverter" />

		<bean class="converters.StringToDemoConverter" />
		<bean class="converters.DemoToStringConverter" />

		<bean class="converters.StringToDescriptionConverter" />
		<bean class="converters.DescriptionToStringConverter" />

		<bean class="converters.StringToResourceConverter" />
		<bean class="converters.ResourceToStringConverter" />


		<bean class="converters.StringToActorConverter" />
		<bean class="converters.ActorToStringConverter" />

		<bean class="converters.StringToAdministratorConverter" />
		<bean class="converters.AdministratorToStringConverter" />


		<bean class="converters.StringToCommentConverter" />
		<bean class="converters.CommentToStringConverter" />
		
		<bean class="converters.InvestmentToStringConverter" />
		<bean class="converters.StringToInvestmentConverter" />
		
		<bean class="converters.InstalmentToStringConverter" />
		<bean class="converters.StringToInstalmentConverter" />


	</util:list>

</beans>